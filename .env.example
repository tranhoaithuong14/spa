# Environment Configuration
NODE_ENV=development

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001
REACT_APP_API_TIMEOUT=10000

# App Configuration
REACT_APP_NAME=React SPA Production
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Production-ready React SPA with TypeScript, Webpack, LESS

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_SERVICE_WORKER=false
REACT_APP_ENABLE_PWA=false

# Security
REACT_APP_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Development
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Build Configuration
GENERATE_SOURCEMAP=true
BUILD_PATH=dist
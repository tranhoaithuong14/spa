# Production Environment
NODE_ENV=production

# API Configuration
REACT_APP_API_BASE_URL=https://api.yourdomain.com
REACT_APP_API_TIMEOUT=15000

# App Configuration
REACT_APP_NAME=React SPA Production
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Production-ready React SPA with TypeScript, Webpack, LESS

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_ENABLE_PWA=true

# Security
REACT_APP_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Development
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error

# Build Configuration
GENERATE_SOURCEMAP=false
BUILD_PATH=dist